{
  "name": "@ssml-editor/example",
  "private": true,
  "version": "1.0.0",
  "description": "all example are settled here",
  "homepage": "https://github.com/zmkn/ssml-editor/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmkn/ssml-editor.git"
  },
  "bugs": {
    "url": "https://github.com/zmkn/ssml-editor/issues"
  },
  "license": "MIT",
  "author": "hz <beijingren@vip.qq.com>",
  "type": "module",
  "exports": {
    ".": {
      "style": "./dist/styles/style.css",
      "import": "./dist/index.es.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs.js"
    },
    "./styles/*": "./dist/styles/*"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --force --cors --mode development",
    "build": "run-s rm:dist type-check build-only",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --noEmit --composite false",
    "rm:dist": "rimraf dist",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@ssml-editor/base": "*",
    "@ssml-editor/core": "*",
    "@ssml-editor/editor-vue": "*",
    "@ssml-editor/modules": "*",
    "@ssml-editor/utils": "*",
    "@ssml-editor/vue": "*",
    "axios": "^1.12.2",
    "axios-mock-adapter": "^2.1.0",
    "element-plus": "^2.11.5",
    "slate-vue3": "^0.8.2",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.15",
    "@types/node": "^22.18.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "gh-pages": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-nested": "^7.0.2",
    "rimraf": "^6.0.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.9.3",
    "unplugin-element-plus": "^0.10.0",
    "vite": "^7.1.11",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^3.1.1"
  }
}
