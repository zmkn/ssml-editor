{
  "name": "@ssml-editor/website",
  "version": "1.0.0",
  "private": true,
  "description": "SSML editor demo website",
  "homepage": "https://github.com/zmkn/ssml-editor/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmkn/ssml-editor.git"
  },
  "bugs": {
    "url": "https://github.com/zmkn/ssml-editor/issues"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start ./ --no-open",
    "start:cn": "docusaurus start ./ --locale zh-Hans --no-open",
    "build": "docusaurus build ./",
    "swizzle": "docusaurus swizzle",
